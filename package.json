{
  "name": "pass-or-fail",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "^1.1.1",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "fontsource-roboto": "^3.0.3",
    "i18next": "^19.8.4",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-chained-backend": "^2.0.1",
    "i18next-http-backend": "^1.0.21",
    "i18next-localstorage-backend": "^3.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "6.12.2",
    "react-i18next": "^11.8.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux-persist": "^6.0.0",
    "sass": "^1.30.0",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1",
    "workbox-background-sync": "^6.0.2",
    "workbox-broadcast-update": "^6.0.2",
    "workbox-cacheable-response": "^6.0.2",
    "workbox-core": "^6.0.2",
    "workbox-expiration": "^6.0.2",
    "workbox-google-analytics": "^6.0.2",
    "workbox-navigation-preload": "^6.0.2",
    "workbox-precaching": "^6.0.2",
    "workbox-range-requests": "^6.0.2",
    "workbox-routing": "^6.0.2",
    "workbox-strategies": "^6.0.2",
    "workbox-streams": "^6.0.2",
    "yup": "^0.32.6"
  },
  "devDependencies": {
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.3.5",
    "jest-axe": "^4.1.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "react-scripts": "4.0.1",
    "source-map-explorer": "^2.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "react-scripts test --bail --findRelatedTests --watchAll=false"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx,ts,tsx}",
      "!src/config/**/*.{js,jsx,ts,tsx}",
      "!src/store/store.js",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/service-worker.js",
      "!src/serviceWorkerRegistration.js",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 91,
        "functions": 98,
        "lines": 98,
        "statements": 98
      }
    },
    "coverageReporters": [
      "text"
    ],
    "snapshotSerializers": []
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
